{
  "name": "hamflow",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "codegen": "panda codegen",
    "dev": "panda codegen && nodemon",
    "build": "bun run build:ssr && bun run build:server",
    "build:ssr": "panda codegen && bunx --bun vike build",
    "build:server": "bun build src/server/index.ts --outfile=./build/server --compile --target=bun-darwin-x64",
    "start:server": "NODE_ENV=production bun run build/index.js",
    "start:prod": "NODE_ENV=production bun run src/server/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "init-db": "bun db:migrate",
    "type-check": "tsc --noEmit",
    "lint": "oxlint src && eslint src --concurrency=auto",
    "lint:fix": "oxlint --fix src && eslint src --fix --concurrency=auto",
    "lint:oxc": "oxlint src",
    "lint:eslint": "eslint src --concurrency=auto",
    "preview": "bunx --bun vike preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:endpoints": "bun run scripts/test-endpoints.ts",
    "test:integration": "vitest run src/__tests__/integration",
    "seed": "bun run scripts/seed-db.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.17",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/cron": "^1.4.1",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.0",
    "@mastra/core": "^0.18.0",
    "ai": "^5.0.59",
    "drizzle-orm": "^0.44.5",
    "elysia": "^1.4.9",
    "elysia-connect-middleware": "^0.0.6",
    "ical-generator": "^9.0.0",
    "jose": "^6.1.0",
    "pino": "^9.12.0",
    "pino-http-print": "^3.1.0",
    "pkce-challenge": "^5.0.0",
    "postgres": "^3.4.7",
    "vike": "^0.4.240",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@ai-sdk/react": "^2.0.59",
    "@ark-ui/react": "^5.25.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/dev": "^1.4.1",
    "@pandacss/eslint-plugin": "^0.2.12",
    "@park-ui/panda-preset": "^0.43.1",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.23",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "19.2",
    "@types/react-dom": "19.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "bun-types": "^1.2.23",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-oxlint": "^1.19.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "lucide-react": "^0.544.0",
    "nodemon": "^3.1.10",
    "oxlint": "^1.19.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.6",
    "postcss-prune-var": "^1.1.2",
    "prettier": "^3.6.2",
    "react": "19.2",
    "react-dom": "19.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "vike-react": "^0.6.6",
    "vite": "^7.1.7",
    "vite-plugin-cjs-interop": "^2.3.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
